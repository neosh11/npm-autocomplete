{
  "name": "@neosh11/autocomplete-search",
  "version": "1.0.0",
  "description": "This package enables you to perform prefix searches with exceptional speed.",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "node esbuild.config.js && tsc --emitDeclarationOnly",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neosh11/npm-autocomplete.git"
  },
  "keywords": [
    "autocomplete",
    "search"
  ],
  "author": "Neosh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neosh11/npm-autocomplete/issues"
  },
  "homepage": "https://github.com/neosh11/npm-autocomplete#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "esbuild": "^0.17.12",
    "jest": "^29.5.0",
    "prettier": "^2.8.6",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",

    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",

    
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.2"
  }
}
